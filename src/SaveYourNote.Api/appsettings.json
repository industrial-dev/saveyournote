{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}{NewLine}    âžœ {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"],
    "Properties": {
      "Application": "SaveYourNote",
      "Environment": "Development"
    }
  },
  "AllowedHosts": "*",
  "WhatsApp": {
    "VerifyToken": "SHOULD_BE_SET_IN_ENVIRONMENT_VARIABLE",
    "AppSecret": "",
    "AccessToken": "SHOULD_BE_SET_IN_ENVIRONMENT_VARIABLE",
    "PhoneNumberId": "SHOULD_BE_SET_IN_ENVIRONMENT_VARIABLE",
    "ApiVersion": "v22.0"  }
}
